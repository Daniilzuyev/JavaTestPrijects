public class FightCatsContinue {

	public static void main(String[] args) {
		
	    FightCats cat1 = new FightCats();
	    cat1.name = "Vaska";
	    cat1.age = 4;
	    cat1.weight = 10;
	    cat1.strength = 6;
	    
	    FightCats cat2 = new FightCats();
	    cat2.name = "Barsik";
	    cat2.age = 6;
	    cat2.weight = 12;
	    cat2.strength = 8;
	    System.out.print(cat1.fight(cat2));
	}
	

}
