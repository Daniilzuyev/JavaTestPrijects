public class MultTable 
{
	 public static void main(String[] args) throws Exception {
		    {
		        int j = 1;
		        int i = 1;
		        while (j<=10){  //1
		        	while (i<=10){   //2
		            System.out.print(i*j+" ");
		            i++;
		        }
		        	j++;
		        	System.out.println(" ");
		        	i = 1; // чтоб выполнялось i = 1, а не 11 после j++
		        }
		    }
		}
}

